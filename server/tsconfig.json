{
  "compilerOptions": {
    "target": "es2022",
    "module": "nodenext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    // "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["src/**/*"],
  // "exclude": ["node_modules"],
  // "moduleResolution": "node",
  "exclude": ["node_modules"]
}
